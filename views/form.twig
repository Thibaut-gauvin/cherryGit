{% extends 'template.html.twig' %}

{% block title %}Overview{% endblock%}

{% block body %}

    <div id="page-wrapper">
        <div class="container-fluid">

            <!-- Page Heading -->
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">
                        Form
                    </h1>
                </div>
            </div>
            <!-- /.row -->

            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <form action="/postApi" method="post">
                        <div class="form-group">
                            <label for="repository">Email address</label>
                            <input type="text" name="repository" class="form-control" id="repository" placeholder="Enter repository name">
                        </div>
                        <div class="form-group">
                            <label for="branch">Password</label>
                            <input type="text" name="branch" class="form-control" id="branch" placeholder="Enter branch name">
                        </div>
                        <div class="form-group">
                            <label for="commit">Password</label>
                            <input type="text" name="commit" class="form-control" id="commit" placeholder="Enter commit hash">
                        </div>
                        <button type="reset" class="btn btn-danger">Reset</button>
                        <button type="submit" class="btn btn-success">Submit</button>
                    </form>
                </div>
            </div>
            <!-- /.row -->

        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->

{% endblock %}